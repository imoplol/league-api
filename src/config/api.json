{
  "api": {
    "NA": {
      "supported": [
        "free-champion",
        "current-game",
        "recent-game",
        "summoner",
        "champion-info"
      ],
      "actions": {
        "free-champion":{
          "variations":[
            "champion",
            "champion/{id}"
          ],
          "options": {
            "path": "/api/lol/na",
            "version": "v1.2"
          },
          "limitrate": true
        },
        "current-game":
        {
          "variations":[
            "getSpectatorGameInfo/NA1/{summonerID}"
          ],
          "options": {
            "path": "/observer-mode/rest/consumer",
            "version": "v1.0"
          },
          "limitrate": true
        },
        "recent-game":
        {
          "variations":[
            "game/by-summoner/{summonerID}/recent"
          ],
          "options": {
            "path": "/api/lol/na",
            "version": "v1.3"
          },
          "limitrate": true
        },
        "summoner":
        {
          "variations":[
            "summoner/by-name/{summonerName}"
          ],
          "options": {
            "path": "/api/lol/na",
            "version": "v1.4"
          },
          "limitrate": true
        },
        "champion-info":
        {
          "variations":[
            "champion",
            "champion/{id}"
          ],
          "options": {
            "path": "/api/lol/static-data/na",
            "version": "v1.2"
          },
          "limitrate": false
        }
      }
    }
  }
}